@{
}
@model List<dotnetCoreMVC_SKA.Controllers.Order>

<table class="table">
    <thead>
        <th>Name</th>
        <th>Price</th>
        <th>Remark</th>
        <th>Date</th>
    </thead>
    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <tbody>
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Price</td>
                    <td>@item.Remark</td>
                    <td>@item.InputTime</td>
                </tr>
            </tbody>
        }
    }

</table>